LMP_VG[41].GAUGE := IG903;
LMP_VG[42].GAUGE := PG722B;
LMP_VG[43].GAUGE := IG909;

FOR i := 1 TO 50 DO
fb_LMP_VG (index := i, astGauges := LMP_VG);
END_FOR

