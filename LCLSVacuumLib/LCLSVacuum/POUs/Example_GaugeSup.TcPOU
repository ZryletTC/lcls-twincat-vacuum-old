(* Upstream vacuum OK signal *)
(* This signal tells the AMO_PLC that the KBP_VGC_02 is OK to open *)
DiscIO_IC_EB1Ch08 := LMP_VG[12].AT_VAC AND LMP_VGC10.OPN_LS;

(* pnCCD Chamber *)
